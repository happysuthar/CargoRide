                        "meth": "POST",
                        "link": "<%= globals.port_base_url %>v1/user/delete-account",
                        "mandatory": "user_id,password",
                        "optional": "",
                        "is_header": "YES",
                        "is_push": "NO",
                        "header": "api_key",
                        "notes": "Deletes the user account after verifying the password.",
                        "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
