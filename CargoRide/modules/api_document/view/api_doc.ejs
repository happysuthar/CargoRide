                                            <v-chip color="blue darken-2" text-color="white">
                                                <v-avatar>
                                                    <v-icon></v-icon>
                                                </v-avatar> EXAMPLE
                                            </v-chip>
                                            <p v-html="message.exp"></p>
                                        </v-alert>
                                        <v-alert v-if="message.notes" :value="true" outline color="white"
                                            style="padding:5px;">
                                            <v-chip color="blue lighten-1" text-color="white">
                                                <v-avatar>
                                                    <v-icon>bookmark</v-icon>
                                                </v-avatar> NOTES
                                            </v-chip>
                                            <span>{{ message.notes }}
                                        </v-alert>
