    <script>
        //console.log('<%= globals.base_url %>enc_dec.php');

        new Vue({
            el: '#app',
            data: () => ({
                messages: [
                    {
                        "color": "teal",
                        "icon": "person",
                        "title": "1 : USER",
                        "name": "Signup",
                        "meth": "POST",
                        "link": "<%= globals.port_base_url %>v1/user/signup",
                        "mandatory": "username,(mobile_number / email),password,login_type('S','F','G','A'),device_type('I','A'),device_token,time_zone,device_name,os_version,app_version",
                        "optional": "full_name,country_code,social_id",
                        "is_header": "YES",
                        "is_push": "NO",
                        "header": "api_key",
                        "notes": "This API is used to register a new user. For social logins ('F', 'G', 'A'), social_id is required instead of password.",
                        "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                        "imp": "Code 0 => Failure (e.g., username/email/mobile exists), Code 1 => Successfully registered"
                    },
                    {
                        "color": "teal",
                        "icon": "person",
                        "title": "2 : USER",
                        "name": "Send OTP",
