                        "mandatory": "user_id",
                        "optional": "",
                        "is_header": "YES",
                        "is_push": "NO",
                        "header": "api_key",
                        "notes": "Retrieves the user's profile information.",
                        "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                        "imp": "Code 0 => Failure (e.g., user not found), Code 1 => Profile retrieved successfully"
                    },
                        {
                            "color": "teal",
                            "icon": "person",
                            "title": "1 : POST",
                            "name": "Get Homepage",
                            "meth": "GET",
                            "link": "<%= globals.port_base_url %>v1/post/homepage",
                            "mandatory": "",
                            "optional": "page,limit",
                            "is_header": "YES",
                            "is_push": "NO",
                            "header": "api_key",
                            "notes": "Fetches posts for the homepage with pagination. Default page is 1, limit is 2.",
                            "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                            "imp": "Code 0 => No posts found, Code 1 => Posts loaded successfully"
                        },
                        {
                            "color": "teal",
                            "icon": "person",
                            "title": "2 : POST",
                            "name": "Get Post Likes",
                            "meth": "GET",
                            "link": "<%= globals.port_base_url %>v1/post/likes",
