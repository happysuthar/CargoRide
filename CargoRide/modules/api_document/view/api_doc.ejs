                            "color": "teal",
                            "icon": "person",
                            "title": "3 : POST",
                            "name": "Get Post Comments",
                            "meth": "GET",
                            "link": "<%= globals.port_base_url %>v1/post/comments",
                            "mandatory": "post_id",
                            "optional": "",
                            "is_header": "YES",
                            "is_push": "NO",
                            "header": "api_key",
                            "notes": "Fetches comments and their replies for a specific post.",
                            "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                            "imp": "Code 0 => No comments found, Code 1 => Comments retrieved successfully"
                        },
                        {
                            "color": "teal",
                            "icon": "person",
                            "title": "4 : POST",
                            "name": "Add Comment to Post",
                            "meth": "POST",
                            "link": "<%= globals.port_base_url %>v1/post/comment",
                            "mandatory": "user_id,post_id,comment",
                            "optional": "",
                            "is_header": "YES",
                            "is_push": "NO",
                            "header": "api_key",
                            "notes": "Adds a comment to a specific post.",
                            "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                            "imp": "Code 0 => Failure (e.g., query error), Code 1 => Comment added successfully"
                        },
