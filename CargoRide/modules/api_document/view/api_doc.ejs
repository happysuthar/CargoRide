                            "icon": "person",
                            "title": "6 : POST",
                            "name": "Reply to Comment",
                            "meth": "POST",
                            "link": "<%= globals.port_base_url %>v1/post/comment/reply",
                            "mandatory": "user_id,comment_id,reply",
                            "optional": "",
                            "is_header": "YES",
                            "is_push": "NO",
                            "header": "api_key",
                            "notes": "Adds a reply to a specific comment.",
                            "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                            "imp": "Code 0 => Failure (e.g., query error), Code 1 => Reply added successfully"
