                        "mandatory": "verify_with('Mobile','Email'),(mobile_number / email),country_code(if Mobile)",
                        "optional": "",
                        "is_header": "YES",
                        "is_push": "NO",
                        "header": "api_key",
                        "notes": "Sends an OTP to the user's mobile or email for verification. OTP expires in 20 minutes.",
                        "status": "<br>1. HTTP_OK[200] - code : 0 <br>2. HTTP_OK[200] - code : 1",
                        "imp": "Code 0 => Failure (e.g., user not found, OTP send failed), Code 1 => OTP sent successfully"
                    },
                    {
                        "color": "teal",
                        "icon": "person",
                        "title": "3 : USER",
                        "name": "Verify User",
                        "meth": "POST",
                        "link": "<%= globals.port_base_url %>v1/user/verify",
                        "mandatory": "verify_with('Mobile','Email'),(mobile_number / email),otp,flag",
                        "optional": "country_code(if Mobile)",
                        "is_header": "YES",
                        "is_push": "NO",
                        "header": "api_key",
                        "notes": "Verifies the user using the OTP sent to their mobile or email. If flag is true, updates user verification status.",
